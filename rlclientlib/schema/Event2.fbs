// EventHubInteraction Schema used by FlatBuffer
include "Metadata.fbs";
include "OutcomeEvent.fbs";
include "TestEvent1.fbs";
include "TestEvent2.fbs";

namespace reinforcement_learning.messages.flatbuff;

union PayloadU {TestEvent1, TestEvent2}

table EventU {
    pass_probability:float;
    key:string;
    Payload:PayloadU;
}

// Collection of Ranking events
table EventBatchU {
    events:[EventU];
}

root_type EventBatchU;
