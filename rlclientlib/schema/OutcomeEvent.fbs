// EventHubInteraction Schema used by FlatBuffer

namespace reinforcement_learning.messages;

table OutcomeEventString {
    event_id:string;
    value:string;
}

table OutcomeEventNumeric {
    event_id:string;
    value:float;
}

table OutcomeEventTaken {
    event_id:string;
}

union OutcomeEventAny { OutcomeEventString, OutcomeEventNumeric, OutcomeEventTaken }

table OutcomeEvent {
    event:OutcomeEventAny;
}

table OutcomeEventBatch {
    events:[OutcomeEvent];
}
